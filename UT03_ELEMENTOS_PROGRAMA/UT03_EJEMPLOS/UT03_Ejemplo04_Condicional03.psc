Proceso UT03_Ejemplo04_Programa_edad
	
	Definir edad Como Entero;
	
	Escribir "Introduzca su edad: ";
	
	Leer edad;
	
	Si edad==18 Entonces
		Escribir "Enhorabuena, ya eres mayor!!!!";
	FinSi
	
	Si edad>=18 Entonces
		Escribir "Bienvenido!!!";
	FinSi
	
	Si edad<18 Entonces
		Escribir "No puedes pasar.";
	FinSi
		
		
FinProceso
